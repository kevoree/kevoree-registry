---
extends:
- eslint:recommended
- angular
env:
  node: true
  browser: true
globals:
  SockJS: true
  Stomp: true
rules:
  wrap-iife:
  - 2
  - inside
  eqeqeq: 2
  no-use-before-define:
  - 2
  - nofunc
  no-unused-vars:
  - 2
  - vars: local
    args: none
  no-multi-str: 2
  no-irregular-whitespace: 2
  semi:
  - 2
  - always
  indent:
  - error
  - tab
  - SwitchCase: 1
  linebreak-style: 0
  eol-last: 2
  angular/json-functions: 0
  angular/di-unused: 2
